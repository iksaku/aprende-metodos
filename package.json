        "@tailwindcss/ui": "^0.1",
        "alpinejs": "^2.0",
        "autoprefixer": "^9.7.2",
        "cross-env": "^7.0",
        "katex": "^0.11.1",
        "laravel-mix": "^5.0",
        "postcss": "^7.0.21",
